@model IList<ProductOverviewModel>

@using Nop.Web.Models.Catalog;
@if (Model.Count > 0)
{
    <div class="related-products-grid product-grid">
        
        @foreach (var item in Model)
        {
            <div class="item-box four columns">@Html.Partial("_ProductBox", item)</div>
        }
    </div>
}
